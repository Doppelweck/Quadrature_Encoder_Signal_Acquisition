var BIOS = xdc.useModule('ti.sysbios.BIOS');
var Startup = xdc.useModule('xdc.runtime.Startup');
var Idle = xdc.useModule('ti.sysbios.knl.Idle');
var Timer = xdc.useModule('ti.sysbios.hal.Timer');
var Memory = xdc.useModule('xdc.runtime.Memory');
var SysMin = xdc.useModule('xdc.runtime.SysMin');
var System = xdc.useModule('xdc.runtime.System');
var Hwi = xdc.useModule('ti.sysbios.hal.Hwi');
Idle.idleFxns[0] = "&Task_Idle";
var timer0Params = new Timer.Params();
timer0Params.instance.name = "timer0";
timer0Params.period = 1000;
Program.global.timer0 = Timer.create(0, "&Task_1ms", timer0Params);
BIOS.cpuFreq.lo = 80000000;
SysMin.flushAtExit = true;
System.extendedFormats = "%$L%$S%$F%f";
var hwi0Params = new Hwi.Params();
hwi0Params.instance.name = "qei0";
Program.global.qei0 = Hwi.create(29, "&ISR_QEI0", hwi0Params);
var hwi2Params = new Hwi.Params();
hwi2Params.instance.name = "qei1";
Program.global.qei1 = Hwi.create(54, "&ISR_QEI1", hwi2Params);
var hwi4Params = new Hwi.Params();
hwi4Params.instance.name = "WTimer1A_Int";
Program.global.WTimer1A_Int = Hwi.create(35, "&ISR_WTimer0A", hwi4Params);
